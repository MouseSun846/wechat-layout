{
  "name": "markdown-to-wechat-editor",
  "version": "1.0.0",
  "description": "A Chrome extension to convert Markdown text to WeChat public account rich text format with Mermaid chart support",
  "main": "background.js",
  "scripts": {
    "test": "echo \"Running tests...\"",
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "start": "http-server . -p 8080",
    "lint": "eslint . --ext .js --config eslint.config.mjs",
    "prepare": "npm run build && npm run lint"
  },
  "keywords": [
    "chrome-extension",
    "markdown",
    "wechat",
    "mermaid"
  ],
  "author": "Developer",
  "license": "MIT",
  "homepage": "https://github.com/your-username/markdown-to-wechat-editor#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/markdown-to-wechat-editor.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/markdown-to-wechat-editor/issues"
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/your-username"
  },
  "dependencies": {
    "marked": "^16.1.2"
  },
  "devDependencies": {
    "eslint": "^9.33.0",
    "http-server": "^14.1.1",
    "webpack": "^5.101.2",
    "webpack-cli": "^6.0.1"
  },
  "files": [
    "dist/",
    "manifest.json",
    "popup.html",
    "styles.css",
    "mermaid.min.js",
    "node_modules/marked/lib/marked.esm.js"
  ]
}
